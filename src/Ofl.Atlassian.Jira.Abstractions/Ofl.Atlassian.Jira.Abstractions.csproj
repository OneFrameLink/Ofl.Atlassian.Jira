  <ItemGroup>
    <PackageReference Include="Ofl.Primitives" Version="3.1.9" />
    <PackageReference Include="Ofl.Reflection" Version="3.0.12" />
    <PackageReference Include="System.Text.Json" Version="4.7.0" />
  </ItemGroup>
</Project>